<!DOCTYPE html>
<html>
<head>
  <title>Flush</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= favicon_link_tag 'favicon.ico' %>
  <%= csrf_meta_tags %>
  <%= javascript_include_tag 'application' %>
   <link href='https://fonts.googleapis.com/css?family=Roboto:400,100,300italic,300,700' rel='stylesheet' type='text/css'>
  <script src="https://widget.cloudinary.com/global/all.js" type="text/javascript"></script>
  <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDXYQNLz_yQujqBcURO2XRoN5ovdKR6r6k&libraries=places"
        type="text/javascript"></script>

<meta name="viewport" content="width=device-width, initial-scale=1.0">

</head>

<body>
  <% if current_user %>
    <script>
      window.CURRENT_USER_ID = <%= current_user.id %>
    </script>
  <% end %>

    <!-- <%#= render partial: "shared/header" %> -->

  <%= yield %>

    <%= render partial: "shared/errors" %>

    <%= render partial: "shared/footer" %>
</body>
</html>
